<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>it.contrader.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>it.contrader.servlets.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>it.contrader.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UsersServlet</servlet-name>
		<servlet-class>it.contrader.servlets.UsersServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TeamServlet</servlet-name>
		<servlet-class>it.contrader.servlets.TeamServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>BiomedicalDataServlet</servlet-name>
		<servlet-class>it.contrader.servlets.BiomedicalDataServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TrainingServlet</servlet-name>
		<servlet-class>it.contrader.servlets.TrainingServlet</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UsersServlet</servlet-name>
		<url-pattern>/UsersServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TeamServlet</servlet-name>
		<url-pattern>/TeamServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BiomedicalDataServlet</servlet-name>
		<url-pattern>/BiomedicalDataServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TrainingServlet</servlet-name>
		<url-pattern>/TrainingServlet</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>4000</session-timeout>
	</session-config>
</web-app>